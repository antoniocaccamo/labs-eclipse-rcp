<plugin>

   <extension
         id="rcp_first_product"
         point="org.eclipse.core.runtime.products">
      <product
            name="rcp.first"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="appName"
               value="rcp.first">
         </property>
         <property
               name="aboutImage"
               value="images/logo.png">
         </property>
      </product>
   </extension>
   <extension
         id="rpcfirst"
         point="org.eclipse.core.runtime.products">
