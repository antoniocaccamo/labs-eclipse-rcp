               name="appName"
               value="rpcfirst">
         </property>
      </product>
   </extension>
   <extension
         id="me_antoniocaccamo_rcp_first_ui"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="First RCP Application">
      </product>
